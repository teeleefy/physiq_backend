{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Goal Schema",
    "required": [
        "memberId",
        "goalName"
    ],
    "properties": {
        "memberId": {
            "type": "integer",
            "title": "The memberId Schema",
            "examples": [
                3
            ]
        },
        "goalName": {
            "type": "string",
            "minLength": 2,
            "maxLength": 100,
            "title": "The goalName Schema",
            "examples": [
                "Make dentist appointment"
            ]
        },
        "goalDetails": {
            "type": "string",
            "maxLength": 150,
            "title": "The goalDetails Schema",
            "examples": [
                "Need to make appointment by end of July. Would like to get a cleaning before end of September."
            ]
        }
    },
    "examples": [{
        "memberId": 3,
        "goalName": "Make dentist appointment",
        "goalDetails": "Need to make appointment by end of July. Would like to get a cleaning before end of September."
    }]
}