{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "memberId",
        "name"
    ],
    "properties": {
        "memberId": {
            "type": "integer",
            "title": "The memberId Schema",
            "examples": [
                3
            ]
        },
        "prescriberId": {
            "type": "integer",
            "title": "The prescriberId Schema",
            "examples": [
                8
            ]
        },
        "name": {
            "type": "string",
           "minLength": 2,
            "maxLength": 100,
            "title": "The name Schema",
            "examples": [
                "Pantoprazole"
            ]
        },
        "startDate": {
            "type": "string",
            "maxLength": 10,
            "title": "The startDate Schema",
            "examples": [
                "2024-04-28"
            ]
        },
        "endDate": {
            "type": "string",
            "maxLength": 10,
            "title": "The endDate Schema",
            "examples": [
                "2024-10-28"
            ]
        },
        "indication": {
            "type": "string",
            "minLength": 2,
            "maxLength": 100,
            "title": "The indication Schema",
            "examples": [
                "Reoccuring heartburn post colon resection."
            ]
        },
        "dose": {
            "type": "string",
            "minLength": 2,
            "maxLength": 100,
            "title": "The dose Schema",
            "examples": [
                "40mg oral twice daily"
            ]
        },
        "notes": {
            "type": "string",
            "minLength": 2,
            "maxLength": 250,
            "title": "The notes Schema",
            "examples": [
                "Take before breakfast and dinner every day for six months. Might cause dry mouth."
            ]
        }
    },
    "examples": [{
        "memberId": 3,
        "prescriberId": 8,
        "name": "Pantoprazole",
        "startDate": "2024-04-28",
        "endDate": "2024-10-28",
        "indication": "Reoccuring heartburn post colon resection.",
        "dose": "40mg oral twice daily",
        "notes": "Take before breakfast and dinner every day for six months. Might cause dry mouth."
    }]
}