{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [],
    "properties": {
        "name": {
            "type": "string",
            "minLength": 2,
            "maxLength": 50,
            "title": "The name Schema",
            "examples": [
                "Migraines"
            ]
        },
        "starDate": {
            "type": "string",
            "maxLength": 10,
            "title": "The startDate Schema",
            "examples": [
                "2024-03-02"
            ]
        },
        "endDate": {
            "type": "string",
            "maxLength": 10,
            "title": "The endDate Schema",
            "examples": [
                "2024-05-02"
            ]
        },
        "notes": {
            "type": "string",
            "maxLength": 250,
            "title": "The notes Schema",
            "examples": [
                "Worsened by poor sleep and stress. Need to make appointment with doctor for migraine medicine."
            ]
        }
    },
    "examples": [{
        "name": "Migraines",
        "startDate": "2024-03-02",
        "endDate": "2024-05-02",
        "notes": "Worsened by poor sleep and stress. Need to make appointment with doctor for migraine medicine."
    }]
}