{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "family_id",
        "first_name",
        "last_name"
    ],
    "properties": {
        "family_id": {
            "type": "integer",
            "title": "The family_id Schema",
            "examples": [
                3
            ]
        },
        "first_name": {
            "type": "string",
            "title": "The first_name Schema",
            "minLength": 2,
            "maxLength": 35,
            "examples": [
                "John"
            ]
        },
        "last_name": {
            "type": "string",
            "minLength": 2,
            "maxLength": 35,
            "title": "The last_name Schema",
            "examples": [
                "Smith"
            ]
        },
        "birthday": {
            "type": "string",
            "default": "",
            "title": "The birthday Schema",
            "examples": [
                "2010-01-01"
            ]
        }
    },
    "examples": [{
        "family_id": 3,
        "first_name": "John",
        "last_name": "Smith",
        "birthday": "2010-01-01"
    }]
}